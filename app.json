{
  "expo": {
    "name": "lokalfinds",
    "slug": "lokalfinds",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/lokalfinds-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/lokalfinds-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location to help you auto-detect your store address when creating or editing a store."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/lokalfinds-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/lokalfinds-logo.png"
    },
    "plugins": [
      "expo-mail-composer"
    ]
  }
}
